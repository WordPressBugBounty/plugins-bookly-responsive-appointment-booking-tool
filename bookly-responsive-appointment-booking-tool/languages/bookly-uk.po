msgstr "ID запису"

msgid "Date of appointment"
msgstr "Дата запису"

msgid "Time of appointment"
msgstr "Час запису"

msgid "Booking number"
msgstr "Номер бронювання"

msgid "Name of category"
msgstr "Назва категорії"

